<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.StatWindow">
   <children>
      <LineChart fx:id="lineChart" layoutX="125.0" layoutY="25.0" prefHeight="351.0" prefWidth="452.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <RadioButton fx:id="weightButton" layoutX="22.0" layoutY="56.0" mnemonicParsing="false" onAction="#weightButtonOnAction" text="Waga" />
      <RadioButton fx:id="bmrButton" layoutX="22.0" layoutY="94.0" mnemonicParsing="false" onAction="#bmrButtonOnAction" text="BMR" />
      <RadioButton fx:id="bmiButton" layoutX="22.0" layoutY="135.0" mnemonicParsing="false" onAction="#bmiButtonOnAction" text="BMI" />
      <Button fx:id="closeButton" layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#closeButtonOnAction" text="Zamknij" />
   </children>
</AnchorPane>
